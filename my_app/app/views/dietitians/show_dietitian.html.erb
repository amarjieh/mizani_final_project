<br>
<br>

<div class="page-header">

  <h3>Dietitian: <%= @dietitian.email %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      


      <dt>Image</dt>
      <dd>
        <img src="<%= @dietitian.image %>">
      </dd>
      
      <dt>Name</dt>
      <dd><%= @dietitian.name %></dd>

      <dt>Country</dt>
      <dd><%= @dietitian.country %></dd>

      <dt>Years of Experience</dt>
      <dd><%= @dietitian.years_of_expertise %></dd>

      <dt>Areas of Expertise</dt>
      <dd><%= @dietitian.areas_of_expertise %></dd>

      <dt>Rate</dt>
      <dd><%= @dietitian.rate %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/sessions" class="btn btn-primary">
        Back
      </a>
      <a href="/sessions/<%= @session.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_session/<%= @session.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>

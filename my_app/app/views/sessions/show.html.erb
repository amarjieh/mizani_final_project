<div class="page-header">

  <h3>Session #<%= @session.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Dietitian</dt>
      <dd><%= @session.dietitian_id %></dd>

      <dt>Time</dt>
      <dd><%= @session.time %></dd>

      <dt>Status</dt>
      <dd><%= @session.status %></dd>

      <dt>Client</dt>
      <dd><%= @session.client_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/sessions" class="btn btn-primary">
        Back
      </a>
      <a href="/sessions/<%= @session.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_session/<%= @session.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
